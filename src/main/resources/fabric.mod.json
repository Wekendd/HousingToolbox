{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",
  "name": "${name}",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Wekend"
  ],
  "contact": {
    "homepage": "https://wekend.dev",
    "sources": "https://github.com/Wekendd/Mop",
    "issues": "https://github.com/Wekendd/Mop/issues"
  },
  "license": "LGPL-3.0",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "dev.wekend.mop.Mop"
      }
    ],
    "modmenu": [
      {
        "adapter": "kotlin",
        "value": "dev.wekend.mop.integrations.ModMenuIntegration"
      }
    ]
  },
  "mixins": [
    "mop.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16",
    "fabric-language-kotlin": ">=1.13.6+kotlin.2.2.20",
    "minecraft": "${minecraft}",
    "yet_another_config_lib_v3": ">=${yacl}"
  }
}